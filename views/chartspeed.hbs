
  <head>

    <div class="boxed"> <!--This is the box that you see on the screen which contains all of the content for the user to view -->

    <link rel="stylesheet" href="public/css/styles.css"></link>
    <!--Load the AJAX API-->
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">

      // Load the Visualization API and the corechart package.
      google.charts.load('current', {'packages':['corechart']});

      // Set a callback to run when the Google Visualization API is loaded.
      google.charts.setOnLoadCallback(drawChart);

      // Callback that creates and populates a data table,
      // instantiates the pie chart, passes in the data and
      // draws it.
      function drawChart() {

        var data = google.visualization.arrayToDataTable([
     ['Element', 'Speed in KM/H', { role: 'style' }],
     ['1st - 10th Min', 17, '#157F1F'],
     ['11th - 20th Min', 20, '#157F1F'],
     ['21st - 30th Min', 14, '#157F1F'],
     ['31st - 45th Min', 15, '#157F1F' ],
     ['46th - 50th Min', 22, '#157F1F' ],
     ['51st - 60th Min', 19, '#157F1F' ],
     ['61st - 70th Min', 19, '#157F1F' ],
     ['71st - 80th Min', 21, '#157F1F' ],
     ['81st - 90th Min', 18, '#157F1F' ],
  ]);

        // Set chart options
        var options = {'title':'Average Speed From Todays Session', titleTextStyle:{fontSize:'15', fontName:'"Avenir"'},
                       'hAxis': {title: 'KM/H', titleTextStyle: {color: 'black',fontSize:'15',fontName:'"Avenir"'}},
                       'vAxis': {minValue:10},
                       'backgroundColor': '#858b93',
                       'colors': ['#157F1F', 'green'],
                       animation:{
    duration: 2000,
    easing: 'out',
    startup: true //This is the new option
  },
                       'width':500,
                       'height':300};


        // Instantiate and draw our chart, passing in some options.
        var chart = new google.visualization.BarChart(document.getElementById('chart_div'));
        chart.draw(data, options);
      }
    </script>

    <!--Div that will hold the bar chart-->
    <div class="col-12"> <!--This determines the columns that all of the content goes into, it positions the content in certain parts of the page so that it all looks professional and arranged -->
      <div class="form-group mx-sm-3 mb-2"> <!-- This helps with the positioning and organisation of the page in general -->
        <p>Your Speed</p> <!-- This is the title of the product -->
        <div id="chart_div"></div>
      </div>

      </div>
    </div>
    <div id="chart_div" style="width: 100%; height: 500px;"></div>
  </head>

